#include <iostream>
using namespace std;

const int SIZE = 6;  // Размер массивов

int main() {
    setlocale(LC_ALL, "rus");
    int A[SIZE], B[SIZE];

    cout << "Введите элементы массива A (размером не больше-" << SIZE << "):" << endl;
    for (int i = 0; i < SIZE; i++) {
        cin >> A[i];
    }

    cout << "Введите элементы массива B (размером не больше-" << SIZE << "):" << endl;
    for (int i = 0; i < SIZE; i++) {
        cin >> B[i];
    }

    cout << "Общие элементы массивов A и B:" << endl;

    for (int i = 0; i < SIZE; i++) {
        for (int j = 0; j < SIZE; j++) {
            if (A[i] == B[j]) {
                cout << A[i] << " ";
                break; 
            }
        }
    }

    cout << endl;

    return 0;
}
